# API Configuration (for future backend integration)
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_API_TIMEOUT=30000

# Maps Integration (for future use)
REACT_APP_GOOGLE_MAPS_API_KEY=your_api_key_here

# AWS S3 Configuration (for file uploads)
REACT_APP_AWS_BUCKET_NAME=your_bucket_name
REACT_APP_AWS_REGION=us-east-1

# Feature Flags
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_CAMERA=true
REACT_APP_ENABLE_OFFLINE_MODE=true

# Application Settings
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png,application/pdf


# Application Version
